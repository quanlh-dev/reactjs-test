{
  "name": "reactjs-test",
  "version": "1.0.0",
  "scripts": {
    "dev": "yarn start",
    "buildCI": "set CI=false react-scripts build",
    "deploy": "cross-env NODE_ENV=production node server",
    "postinstall": "node ./bin/postInstall && husky install",
    "start": "vite",
    "build": "vite build",
    "test": "vite test",
    "start:prod": "yarn run build && serve -s build",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "yarn run eslint src",
    "lint:fix": "yarn run eslint --fix src",
    "prettify": "prettier --write"
  },
  "dependencies": {
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.16.7",
    "axios": "^0.21.1",
    "formik": "^2.4.6",
    "i18next": "21.9.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "patch-package": "^6.5.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "1.3.0",
    "react-i18next": "11.18.6",
    "react-json-view": "^1.21.3",
    "redux": "^4.0.5",
    "react-redux": "7.2.8",
    "redux-injectors": "1.3.0",
    "@reduxjs/toolkit": "1.8.5",
    "react-router-dom": "6.10.0",
    "redux-saga": "1.2.1",
    "web-vitals": "2.1.4",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@hot-loader/react-dom": "^16.13.0",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.194",
    "@types/node": "^14.18.27",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.24",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.8.4",
    "redux-devtools-extension": "^2.13.9",
    "sass": "^1.60.0",
    "ts-node": "10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.2.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/messages.ts",
      "!src/**/*/types.ts",
      "!src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
